{
	"compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "out",
    "lib": ["ES2020", "DOM"],
    "types": ["reflect-metadata"],
    "moduleResolution": "node",
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true
  },
	"include": ["src"],
	"outDir": "./out",
	"exclude": ["node_modules", ".vscode-test"]
}
